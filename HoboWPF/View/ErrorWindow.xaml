<Window x:Class="HoboWPF.View.ErrorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HoboWPF.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="ErrorWindow" Height="200" Width="400">
    <Window.Resources>
        <ResourceDictionary Source="/HoboWPF;component/View/Styles/MainHoboWindowStyles.xaml"></ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TextBlock x:Name="Error" Style="{StaticResource Properties_Style}" TextWrapping="Wrap"></TextBlock>
    </Grid>
</Window>
